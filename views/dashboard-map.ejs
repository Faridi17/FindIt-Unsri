<script>
  // Inisialisasi peta di sekitar kampus UNSRI Indralaya
  const map = L.map('map').setView([-3.217, 104.65], 16);

  // Tambahkan layer peta dari OpenStreetMap
  L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
    maxZoom: 19,
    attribution: '&copy; OpenStreetMap contributors'
  }).addTo(map);

  // Data fakultas UNSRI
  const fakultas = [
    { name: "Fakultas Ekonomi", lat: -3.2179922948347626, lng: 104.65064090892372 },
    { name: "Fakultas Hukum", lat: -3.2152647292845398, lng: 104.65079213961201 },
    { name: "Fakultas Teknik", lat: -3.2173254776649243, lng: 104.64614820884489 },
    { name: "Fakultas Kedokteran", lat: -3.2159599702186936, lng: 104.64695748632029 },
    { name: "Fakultas Pertanian", lat: -3.219581769304781, lng: 104.64816467407553 },
    { name: "Fakultas Keguruan dan Ilmu Pendidikan", lat: -3.221087991803207, lng: 104.65166674663436 },
    { name: "Fakultas Ilmu Sosial dan Ilmu Politik", lat: -3.2164920814680253, lng: 104.65110736631193 },
    { name: "Fakultas MIPA", lat: -3.218442677831443, lng: 104.6463383862676 },
    { name: "Fakultas Ilmu Komputer", lat: -3.219481150172853, lng: 104.65115074590034 },
    { name: "Fakultas Kesehatan Masyarakat", lat: -3.2136355601985773, lng: 104.64461449546863 }
  ];

  // Data jumlah barang (diambil dari backend templating)
  const items = [
    <% items.forEach(item => { %>
    { location: "<%= item.location %>", amount: <%= item.amount %> },
    <% }) %>
  ];

  // Fungsi untuk mencari jumlah barang berdasarkan lokasi fakultas
  function getJumlahBarang(namaFakultas) {
    const found = items.find(i => i.location === namaFakultas);
    return found ? found.amount : 0;
  }

  // Tambahkan marker untuk setiap fakultas dengan jumlah barang
  fakultas.forEach(f => {
    const jumlah = getJumlahBarang(f.name);
    L.marker([f.lat, f.lng])
      .addTo(map)
      .bindPopup(`
      <div style="text-align:center;">
        <b>${f.name}</b><br>${jumlah} Barang
      </div>
    `);
  });


  // Sesuaikan tampilan peta agar semua marker terlihat
  const bounds = L.latLngBounds(fakultas.map(f => [f.lat, f.lng]));
  map.fitBounds(bounds);
</script>